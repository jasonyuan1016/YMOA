@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div class="topPanel">
    <div class="toolbar">
        <div class="btn-group">
            <a id="NF-add" authorize="yes" class="btn btn-primary dropdown-text" onclick="btn_back()">返回</a>
        </div>
    </div>
    <div class="search">
        <table>
            <tr>
                @*<td>
                    <a id="NF-add" authorize="yes" class="btn btn-primary dropdown-text" onclick="btn_back()">返回</a>
                </td>*@
                <td>
                    <div class="input-group">
                        <input id="StartTime" type="text" class="form-control input-wdatepicker" style="width: 120px;" onfocus="WdatePicker()">——
                    </div>
                </td>
                <td>
                    <div class="input-group">
                        <input id="EndTime" type="text" class="form-control input-wdatepicker" style="width: 120px;" onfocus="WdatePicker()">
                    </div>
                </td>
                <td>
                    <div class="input-group">
                        <span class="input-group-btn">
                            <button id="btn_search" type="button" class="btn  btn-primary"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="gridPanel">
    <table id="gridList"></table>
    <div id="gridPager"></div>
</div>
@section scripts {
    @Scripts.Render("~/Content/js/datepicker/WdatePicker.js", "~/Content/bootstrap-select/js/bootstrap-select.min.js", "~/Content/bootstrap-select/js/i18n/defaults-zh_CN.min.js")
}
